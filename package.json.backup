{
  "name": "manage-workspace-homepage",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "autoprefixer": "^10.4.14",
    "axe-cli": "^3.2.1",
    "axios": "^1.11.0",
    "compression-webpack-plugin": "^11.1.0",
    "cypress": "^14.5.3",
    "dns2": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "html-validate": "^9.7.1",
    "jest": "^27.5.1",
    "lighthouse": "^12.8.1",
    "lighthouse-ci": "^1.13.1",
    "nodemon": "^3.1.10",
    "pa11y": "^9.0.0",
    "performance-now": "^2.1.0",
    "playwright": "^1.54.2",
    "postcss": "^8.4.24",
    "prettier": "^3.6.2",
    "puppeteer": "^24.15.0",
    "serve": "^14.2.4",
    "snyk": "^1.1298.2",
    "ssl-checker": "^2.0.10",
    "stylelint": "^16.23.0",
    "stylelint-config-standard": "^39.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vercel": "^44.7.0",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:performance && npm run test:a11y",
    "test:unit": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:performance": "lighthouse-ci autorun",
    "test:a11y": "axe-cli --exit --chrome-options="--headless" http://localhost:3000",
    "test:pa11y": "pa11y-ci",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:analyze-serve": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --port 8888",
    "audit:security": "npm audit && snyk test",
    "lint:css": "stylelint "src/**/*.css"",
    "lint:css:fix": "stylelint "src/**/*.css" --fix",
    "validate:html": "html-validate build/index.html",
    "format": "prettier --write "src/**/*.{js,jsx,ts,tsx,css,md}"",
    "format:check": "prettier --check "src/**/*.{js,jsx,ts,tsx,css,md}"",
    "serve:build": "serve -s build -l 3000",
    "test:lighthouse": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "test:bundle-size": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js --mode=static --report",
    "test:responsive": "playwright test tests/responsive.spec.js",
    "test:accessibility": "playwright test tests/accessibility.spec.js",
    "test:performance:budget": "lighthouse-ci autorun --budget-path=./lighthouse-budget.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
